

cc_library_shared {
    name: "libipanat",

    header_libs: ["device_kernel_headers"],

    srcs: [
        "src/ipa_nat_drv.c",
        "src/ipa_nat_drvi.c",
    ],

   shared_libs:
        ["libcutils",
        "libdl",
        "libbase",
        "libutils",
    ],
    export_include_dirs: ["inc"],
    vendor: true,
    cflags: [
        "-Wall",
        "-Werror",
        "-Wno-error=unused-parameter",
    ] + ["-DFEATURE_IPA_ANDROID"],

    product_variables: {
        debuggable: {
            cflags: ["-DDEBUG"],
        },
    },

    clang: true,
}
